{
  "name": "simple-pjax",
  "version": "0.0.10",
  "main": "lib/simple-pjax.js",
  "scripts": {
    "gulp": "gulp",
    "start": "gulp",
    "prepublish": "uglifyjs lib/simple-pjax.js > lib/simple-pjax.min.js -m --lint",
    "build-prod": "gulp build --prod && node ./bundle --prod",
    "serve-prod": "npm run build-prod && gulp server",
    "push": "npm run build-prod && (cd gh-pages && git add -A . && git commit -a -m autocommit && git push origin gh-pages)"
  },
  "description": "Zero-configuration PJAX for typical websites",
  "authors": [
    "Mitranim <me@mitranim.com>"
  ],
  "keywords": [
    "pjax"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mitranim/simple-pjax.git"
  },
  "devDependencies": {
    "browser-sync": "^2.8.0",
    "font-awesome": "^4.3.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-babel": "^5.1.0",
    "gulp-filter": "^2.0.2",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-marked": "^1.0.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-rimraf": "^0.1.1",
    "gulp-sass": "^2.0.4",
    "gulp-statil": "0.0.4",
    "gulp-typescript": "^2.8.0",
    "gulp-watch": "^4.3.3",
    "highlight.js": "^8.6.0",
    "nodemon": "^1.3.8",
    "react": "^0.13.3",
    "stylific": "0.8.3",
    "systemjs": "^0.18.4",
    "systemjs-builder": "^0.12.2",
    "uglify-js": "^2.4.23",
    "yargs": "^3.15.0"
  }
}
